#include<iostream>
using namespace std;
int* inverse (int* arr, int n){
int* inv = new int[n];
for (int i = 0 ; i < n; i++) {
int pos = arr [i];
inv [pos] = i;
}
return inv;
}
int main(){
	int n;
	cin>> n;
	int* arr=new int[n];
	for(int i=0;i<n;i++){
		cin>>arr[i];
	}
	int* inv=inverse(arr,n);
return 0;

}
